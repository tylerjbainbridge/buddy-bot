datasource db {
    provider = "postgres"
    url = "***"
    default  = true
}

generator photon {
    provider = "photonjs"
}

model Guild {
    id String @id

    name String

    user User[]
    commands Command[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model Channel {
    id String @id

    guild Guild

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model Command {
    id String @id

    trigger String
    response String

    user User
    guild Guild

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model User {
    id String @id

    guild Guild
    commands Command[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}